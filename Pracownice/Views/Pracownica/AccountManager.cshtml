@model Pracownice.Models.Pracownica

@{
    ViewBag.Title = "AccountMenager";
}


<link href="../../Content/_MenuAccountManager.css" rel="stylesheet" type="text/css" />
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>


<div class="Menu_AccountMenager">
     <div id="menu">
		<div class="zakladka" style="margin-bottom:35px">Twoje Dane
            <div class="menuGrafikaLinia">
                <img src="../../Content/Images/profileScale.png" style ="margin-top:-20px; margin-left:-50px">
            </div>
        </div>   
			@Html.Partial("../AccountMenager/_PartialTwojeDane",Model)
        
		<div class="zakladka" style="margin-bottom:35px">Twoje Zdjęcia
            <div class="menuGrafikaLinia">
                <img src="../../Content/Images/profileScale.png" style ="margin-top:-20px; margin-left:-50px">
            </div>
        </div>   
		<ul id="mSlider">
			    @Html.Partial("../AccountMenager/_PartialTwojeZdjecia_Profilowe", Model)
                @Html.Partial("../AccountMenager/_PartialTwojeZdjecia_Galeria", Model)
		</ul>

		<div class="zakladka" style="margin-bottom:35px">Twoje Usługi
            <div class="menuGrafikaLinia">
                <img src="../../Content/Images/profileScale.png" style ="margin-top:-20px; margin-left:-50px">
            </div>
        </div>   
		<ul id="mSlider">
			 @Html.Partial("../AccountMenager/_TwojeUslugi", Model)
		</ul>
	</div>
</div>
